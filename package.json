{
  "name": "codeeditor-app",
  "version": "1.0.0",
  "main": "index.js",
  "private": true,
  "engines": {
    "yarn": ">=1.7.0 <2",
    "node": ">=20"
  },
  "scripts": {
    "bundle": "npm run rebuild && theia build --mode development",
    "rebuild": "theia rebuild:browser --cacheRoot ..",
    "start": "theia start",
    "watch": "npm run rebuild && theia build --watch --mode development"
  },
  "theia": {
    "target": "browser",
    "frontend": {
      "config": {
        "applicationName": "CodeEditor"
      }
    }
  },
  "dependencies": {
    "@theia/core": "^1.63.3",
    "@theia/editor": "^1.63.3",
    "@theia/filesystem": "^1.63.3",
    "@theia/markers": "^1.63.3",
    "@theia/messages": "^1.63.3",
    "@theia/monaco": "^1.63.3",
    "@theia/navigator": "^1.63.3",
    "@theia/preferences": "^1.63.3",
    "@theia/process": "^1.63.3",
    "@theia/terminal": "^1.63.3",
    "@theia/workspace": "^1.63.3",
    "@theia/plugin-ext": "^1.63.3",
    "@theia/plugin-ext-vscode": "^1.63.3",
    "@theia/vsx-registry": "^1.63.3"
  },
  "author": "",
  "license": "ISC",
  "description": "CodeEditor - A web-based IDE built on Eclipse Theia",
  "workspaces": [
    "browser-app",
    "electron-app"
  ],
  "devDependencies": {
    "@theia/cli": "^1.63.3",
    "lerna": "^8.2.3"
  }
}
